pipeline {
    agent any
    
    tools {nodejs "node"}
    
    stages {
        stage('Cloning Git 1') {
            steps {
                git 'https://github.com/pablosiegers/DOTT'
            }
        }
        stage('Install Dependencies'){
            steps {
                sh |
                    'echo "Holla"'
                    'npm install'
            }
        }
        stage ('Test') {
            steps {
                sh 'npm test'
            }
        }
    }
}
